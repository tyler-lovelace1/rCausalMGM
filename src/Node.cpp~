#include "Node.hpp"

bool operator==(const Node& n1, const Node& n2) {
    return n1.name == n2.name && n1.type == n2.type;
}

bool operator!=(const Node& n1, const Node& n2) {
    return !(n1 == n2);
}

bool operator< (const Node& n1, const Node& n2) {
    return n1.name < n2.name;
}

bool operator<=(const Node& n1, const Node& n2) {
    return n1 < n2 || n1 == n2;
}

bool operator> (const Node& n1, const Node& n2) {
    return n1.name > n2.name;
}

bool operator>=(const Node& n1, const Node& n2) {
    return n1 > n2 || n1 == n2;
}
